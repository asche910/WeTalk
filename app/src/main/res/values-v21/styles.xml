<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:windowActivityTransitions">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">true</item>
        <item name="android:windowSharedElementsUseOverlay">true</item>
        <item name="android:windowContentTransitions">true</item>

        <item name="divideView">@color/coloreee</item>
        <item name="divideViewDark">@color/coloreee</item>
        <item name="textNormal">@color/blackMiddle</item>
        <item name="blueNight">@color/blueLight</item>
        <item name="whiteNight">@color/white</item>
        <item name="dddNight">@color/grayLight</item>
        <item name="eeeNight">@color/coloreee</item>

    </style>
</resources>